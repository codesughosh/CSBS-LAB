# Generate 1000 samples from a uniform continuous distribution between 0 and 1. Plot the PDF (Probability Density Function) using a histogram and overlay the theoretical PDF using python program

import numpy as np
import matplotlib.pyplot as plt

samples = np.random.uniform(0,1,1000)
plt.hist(samples, bins=30, density=True, alpha=0.6, color='skyblue', edgecolor='black', label="Emperical PDF")
x = np.linspace(0,1,100)
y = np.ones_like(x)
plt.plot(x,y,'r-',linewidth=2,label="Theoretical PDF")
plt.title("PDF of Uniform(0,1) Distribution")
plt.xlabel("Value")
plt.ylabel("Density")
plt.legend()
plt.grid(True)
plt.show()
